{
    "manifest_version": 3,
    "name": "Input Focus",
    "version": "0.9",
    "description": "Focus directly to the input field.",
    "permissions": ["activeTab"],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_icon": "images/icon19.png"
    }
}
